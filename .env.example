# Database Configuration (Docker context)
[REDACTED_DATABASE_URL]
POSTGRES_USER=admin
[REDACTED_POSTGRES_PASSWORD]
POSTGRES_DB=tattoo_studio

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
FLASK_APP=app.py
[REDACTED_FLASK_SECRET_KEY]
HOST=0.0.0.0
PORT=5000

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
[REDACTED_GOOGLE_CLIENT_SECRET]
OAUTHLIB_INSECURE_TRANSPORT=1
OAUTHLIB_RELAX_TOKEN_SCOPE=1

# Jotform Integration
[REDACTED_JOTFORM_API_KEY]
JOTFORM_FORM_ID=your-jotform-form-id

# Optional Google Calendar Settings
GOOGLE_CALENDAR_ID=your-calendar-id@group.calendar.google.com
[REDACTED_GOOGLE_API_KEY]
GOOGLE_SERVICE_ACCOUNT_FILE=/path/to/service-account.json
